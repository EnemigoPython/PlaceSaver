{
    "name": "Place Saver",
    "description": "Save a place on a web page",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "/src/popup.html",
      "default_icon": {
        "16": "/icons/book16.png",
        "32": "/icons/book32.png",
        "48": "/icons/book64.png",
        "128": "/icons/book128.png"
      },
      "icons": {
        "16": "/icons/book16.png",
        "32": "/icons/book32.png",
        "48": "/icons/book64.png",
        "128": "/icons/book128.png"
      }
    }
  }